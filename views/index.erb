<div class="container-fluid">
  <div class="row">
    <table>
      <tr>
        <th>
          Latitude
        </th>
        <th>
          Longitude
        </th>
        <th>
          Address
        </th>
      </tr>
      <% address_list.each do |address| %>
        <tr>
          <td>
            <%= address.lat %>
          </td>
          <td>
            <%= address.lng %>
          </td>
          <td>
            <%= address.full_address %>
          </td>
        </tr>
      <% end %>
    </table>
  </div>
</div>
